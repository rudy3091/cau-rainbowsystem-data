{
  "name": "cau-rainbowsystem-data",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean:data": "rm dist/data/chunk*",
    "clean": "rm -rf dist",
    "copy:data": "cp src/data/bigyogwadata.json dist/data/bigyogwadata.json",
    "build": "swc src -d dist -C module.type=commonjs && npm run copy:data",
    "start:data": "npm run build && node dist/scripts/json-manip.js",
    "start:dev": "nodemon --watch src --delay 0.5 --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start": "npm run build && node dist/index.js",
    "test": "NODE_ENV=test jest --forceExit"
  },
  "_moduleAliases": {
    "@routes": "dist/routes",
    "@data": "dist/data"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.36",
    "@swc/core": "^1.2.52",
    "@swc/jest": "^0.1.2",
    "@types/jest": "^26.0.22",
    "@types/supertest": "^2.0.11",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0"
  }
}
